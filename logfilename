
library(descriptr, pos=26)
# Screen the dataset
descriptr::ds_screener(cabbages)
# Quick summary of whole dataset
summary(cabbages)
library(lattice, pos=27)
library(survival, pos=27)
library(Formula, pos=27)
library(Hmisc, pos=27)
# Summary of all variables
Hmisc::describe(cabbages)
library(tidyverse, pos=31)
library(psych, pos=39)
# Summary of numeric variables
cabbages %>% 
  dplyr::select_if(is.numeric) %>% 
  psych::describe(quant = c(.05, .25, .75, .95),  IQR = TRUE, trim = .10) 
cabbages_summary_2 <- do_summary(~ VitC, data = cabbages)
print(cabbages_summary_2, digits = -1)
remove(cabbages_summary_2)
cabbages_summary_2 <- do_summary(~ VitC, data = cabbages)
print(cabbages_summary_2, digits = 2)
remove(cabbages_summary_2)

